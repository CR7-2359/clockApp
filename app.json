{
  "expo": {
    "name": "myDemo",
    "orientation": "default",
    "slug": "myDemo",
    "version": "1.0.0",
    "icon": "./assets/images/icon.png",
    "scheme": "mydemo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "backgroundColor": "#000000",
      "supportsTablet": true
    },
    "android": {
      "backgroundColor": "#000000",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.cr72359.myDemo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9950b038-f69c-4b48-a6aa-f0a1c1538767"
      }
    }
  }
}
